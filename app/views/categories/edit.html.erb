<%= form_for @category do |f| %>

    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>

    <div>
      <%= f.label :image %>
      <%= f.text_field :image %>
    </div>

    <div>
      <%= f.label :rank %>
      <%= f.text_field :rank %>
    </div>

    <div>
      <%= f.label :id %>
      <%= f.text_field :id %>
    </div>





    <%= f.submit "Edit Category", "class" => "btn btn-primary" %>

<% end %>

<%= link_to "Back to items", items_path, "class" => "btn btn-primary" %>
