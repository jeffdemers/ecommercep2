
<%= form_for @seller, url: create_session_path do |f| %>

    <% flash.each do |key,value|  %>
        <div class="<%= key %>" > <%= value %> </div>
    <% end %>

    <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
    </div>

    <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div>
    <div>
    <%= f.submit "Login" %>
    </div>

<% end %>